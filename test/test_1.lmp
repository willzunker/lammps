units           metal
dimension       3
boundary        p m p
atom_style      atomic
processors      * * *

#lattice         diamond 5.431
#region          box block 0 6 0 6 0 6 units lattice
#create_box      1 box
#create_atoms    1 box
#group           vac id 169
#delete_atoms    group vac
#write_data      silicon-vac_6x6x6x5.431.ldata

read_data       silicon-vac_6x6x6x5.431.ldata

pair_style      sw
pair_coeff      * * ../potentials/Si.sw Si
mass            1 28.086
#mass            1 0.00291111

neighbor        0.44 bin
neigh_modify    delay 5

variable        peratom equal pe/atoms

thermo          1
thermo_style    custom step dt time fnorm fmax pe v_peratom ke temp
#thermo_modify   format float %1.18e

timestep        0.001

#velocity        all create 3.0 456789123 
#fix             1 all nve
#run             1
#unfix           1

min_style       adaptglok
minimize        0.0 1e-8 1000 10000
run             0

#fix             1 all nve
#run             20
